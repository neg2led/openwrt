#include "qcom-ipq8064-v2.0.dtsi"
#include <dt-bindings/interrupt-controller/arm-gic.h>
#include <dt-bindings/interrupt-controller/irq.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	model = "Sophos APX530";
	compatible = "sophos,apx530", "qcom,ipq8064";

	memory@0 {
		reg = <0x42000000 0x3C000000>;
		device_type = "memory";
	};

	reserved-memory {
		#address-cells = <1>;
		#size-cells = <1>;
		ranges;
		rsvd@41200000 {
			reg = <0x41200000 0x300000>;
			no-map;
		};
	};

	aliases {
		led-boot = &led_status_green;
		led-failsafe = &led_status_red;
		led-running = &led_status_green;
		led-upgrade = &led_status_red;

		serial0 = &gsbi4_serial;
		mdio-gpio0 = &mdio0;
		ethernet0 = &gmac3;
		ethernet1 = &gmac2;
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-0 = <&led_pins>;
		pinctrl-names = "default";

		led_status_red: status_red {
			label = "red:status";
			gpios = <&qcom_pinmux 27 GPIO_ACTIVE_HIGH>;
		};

		led_status_green: status_green {
			label = "green:status";
			gpios = <&qcom_pinmux 28 GPIO_ACTIVE_HIGH>;
		};

	};

	keys {
		compatible = "gpio-keys";

		pinctrl-0 = <&button_pins>;
		pinctrl-names = "default";

		reset {
			label = "reset";
			gpios = <&qcom_pinmux 60 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
			debounce-interval = <60>;
			wakeup-source;
		};
	};

	i2c-gpio {
		#address-cells = <1>;
		#size-cells = <0>;

		compatible = "i2c-gpio";
		pinctrl-0 = <&i2c_pins>;
		gpios = <&qcom_pinmux 8 GPIO_ACTIVE_HIGH>, /* sda */
			<&qcom_pinmux 9 GPIO_ACTIVE_HIGH>; /* scl */
		i2c-gpio,delay-us = <2>; /* ~100kHz */


		tpm@29 {
			reg = <0x29>;
			compatible = "atmel,at97sc3204t";
			reset-gpio = <&qcom_pinmux 59 GPIO_ACTIVE_HIGH>;
		};
	};
};

&adm_dma {
	status = "okay";
};

&gmac2 {
	status = "okay";
	phy-mode = "sgmii";
	qcom,id = <2>;
	qcom,phy-mdio-addr = <1>;
	local-mac-address = [00 00 00 00 00 00];

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&gmac3 {
	status = "okay";
	phy-mode = "sgmii";
	qcom,id = <3>;
	qcom,phy-mdio-addr = <2>;
	local-mac-address = [00 00 00 00 00 00];

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&gsbi2 {
	qcom,mode = <GSBI_PROT_I2C_UART>;
	status = "okay";

	gsbi2_serial: serial@12490000 {
		status = "okay";
	};
};

&gsbi4 {
	qcom,mode = <GSBI_PROT_I2C_UART>;
	status = "okay";

	gsbi4_serial: serial@16340000 {
		status = "okay";
	};
};

&gsbi5 {
	status = "okay";
	qcom,mode = <GSBI_PROT_SPI>;

	spi4: spi@1a280000 {
		status = "okay";

		pinctrl-0 = <&spi_pins>;
		pinctrl-names = "default";
		cs-gpios = <&qcom_pinmux 20 GPIO_ACTIVE_HIGH>;

		flash: flash@0 {
			compatible = "s25fl256s1";
			#address-cells = <1>;
			#size-cells = <1>;
			reg = <0>;
			m25p,fast-read;

			partitions {
				compatible = "fixed-partitions";
				#address-cells = <1>;
				#size-cells = <1>;

				partition@0 {
					label = "SBL1";
					reg = <0x0 0x10000>;
					read-only;
				};

				partition@10000 {
					label = "MIBIB";
					reg = <0x10000 0x20000>;
					read-only;
				};

				partition@30000 {
					label = "SBL2";
					reg = <0x30000 0x30000>;
					read-only;
				};

				partition@60000 {
					label = "SBL3";
					reg = <0x60000 0x30000>;
					read-only;
				};

				partition@90000 {
					label = "DDRCONFIG";
					reg = <0x90000 0x10000>;
					read-only;
				};

				partition@a0000 {
					label = "APPSBLENV";
					reg = <0xa0000 0x10000>;
				};

				art: partition@b0000 {
					label = "ART";
					reg = <0xb0000 0x10000>;
					read-only;
				};

				partition@c0000 {
					label = "TZ";
					reg = <0xc0000 0x30000>;
					read-only;
				};

				partition@f0000 {
					label = "RPM";
					reg = <0xf0000 0x20000>;
					read-only;
				};

				partition@110000 {
					label = "APPSBL";
					reg = <0x110000 0x90000>;
					read-only;
				};

				partition@1a0000 {
					label = "stat_cfg";
					reg = <0x1a0000 0x60000>;
					read-only;
				};

				partition@200000 {
					label = "dyn_cfg";
					reg = <0x200000 0x100000>;
					read-only;
				};

				partition@300000 {
					label = "dyn_cfg_backup";
					reg = <0x300000 0x100000>;
					read-only;
				};
			};
		};
	};
};

&hs_phy_0 {
	status = "disabled";
};

&ss_phy_0 {
	status = "disabled";
};

&hs_phy_1 {
	status = "disabled";
};

&ss_phy_1 {
	status = "disabled";
};

&mdio0 {
	status = "okay";

	pinctrl-0 = <&mdio0_pins>;
	pinctrl-names = "default";

	phy0: ethernet-phy@0 {
		device_type = "ethernet-phy";
		reg = <1>;
	};

	phy1: ethernet-phy@1 {
		device_type = "ethernet-phy";
		reg = <2>;
	};
};

&nand_controller {
	status = "okay";

	pinctrl-0 = <&nand_pins>;
	pinctrl-names = "default";

	nand@0 {
		reg = <0>;
		compatible = "qcom,nandcs";

		nand-ecc-strength = <4>;
		nand-bus-width = <8>;
		nand-ecc-step-size = <512>;

		partitions {
			compatible = "qcom,smem-part";
		};
	};
};

&pcie0 {
	status = "okay";
	perst-gpio = <&qcom_pinmux 3 GPIO_ACTIVE_HIGH>;
};

&pcie1 {
	status = "okay";
	perst-gpio = <&qcom_pinmux 48 GPIO_ACTIVE_HIGH>;
};

&rpm {
	pinctrl-0 = <&i2c4_pins>;
	pinctrl-names = "default";
};

&qcom_pinmux {

	spi_pins: spi_pins {
		mux {
			pins = "gpio18", "gpio19", "gpio21";
			function = "gsbi5";
			drive-strength = <10>;
			bias-none;
		};
		cs {
			pins = "gpio20";
			drive-strength = <12>;
		};
	};

	nand_pins: nand_pins {
		mux {
			pins = "gpio34", "gpio35", "gpio36",
				"gpio37", "gpio38", "gpio39",
				"gpio40", "gpio41", "gpio42",
				"gpio43", "gpio44", "gpio45",
				"gpio46", "gpio47";
			function = "nand";
			drive-strength = <10>;
			bias-disable;
		};
		pullups {
			pins = "gpio39";
			bias-pull-up;
		};
		hold {
			pins = "gpio40", "gpio41", "gpio42",
				"gpio43", "gpio44", "gpio45",
				"gpio46", "gpio47";
			bias-bus-hold;
		};
	};

	hs_uart_pins: hs_uart_pins {
		mux {
			pins = "gpio22", "gpio23", "gpio24", "gpio25" ;
			function = "gsbi2";
			drive-strength = <12>;
			bias-disable;
		};
	};

	led_pins: led_pins {
		mux {
			pins = "gpio27", "gpio28";
			function = "gpio";
			drive-strength = <2>;
			bias-pull-down;
			output-low;
		};
	};

	button_pins: button_pins {
		mux {
			pins = "gpio60";
			function = "gpio";
			drive-strength = <2>;
			bias-pull-up;
		};
	};

	i2c_pins: i2c_pins {
		mux {
			pins = "gpio8", "gpio9";
			function = "gpio";
			drive-open-drain;
		};
	};
};

&tcsr {
	status = "okay";
};

